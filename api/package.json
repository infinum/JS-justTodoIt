    "@tsed/exceptions": "^6.133.2",
    "@tsed/platform-express": "^6.133.2",
    "@tsed/schema": "^6.133.2",
    "@tsed/swagger": "^6.133.2",
    "@tsed/typeorm": "^6.133.2",
    "@types/swagger-schema-official": "^2.0.25",
    "ajv": "^8.17.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.21.2",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^3.5.0",
    "sqlite3": "^5.1.7",
    "tslib": "^2.8.1",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "^3.26.3",
    "@tsed/cli-plugin-typeorm": "^3.26.3",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
